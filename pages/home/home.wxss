/* pages/carddetail/carddetail.wxss */

page {
  height: 100%;
  /* background: #f7f7fa; */
  position: relative;
  background: #f2f2f2;
}

.form {
  position: absolute;
  z-index: 10;
  right: 40rpx;
  top: 55px;
  border: 2rpx solid #ccc;
  padding: 20rpx;
}

.form button {
  color: red;
}

.card {
  padding: 0 32rpx;
  padding-top: 104rpx;
  background: #fff;
}

.card .card-info {
  width: 100%;
  height: 442rpx;
  background: #fff;
  box-shadow: 0 4rpx 32rpx 0 rgba(0, 0, 0, 0.12);
  border-radius: 8rpx;
  padding-left: 32rpx;
  position: relative;
  box-sizing: border-box;
}

.card .card-info.active {
  height: 500rpx;
}

.img-box {
  position: absolute;
  left: 32rpx;
  top: -72rpx;
}

.img-box .avatar {
  width: 144rpx;
  height: 144rpx;
  border-radius: 50%;
}

.img-box .grade {
  width: 44rpx;
  height: 44rpx;
  position: absolute;
  right: 0;
  bottom: 0;
}

.edit {
  position: absolute;
  right: 32rpx;
  top: 16rpx;
  color: #007aff;
  font-size: 12px;
  display: flex;
  align-items: center;
}

.edit image {
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
  margin-right: 8rpx;
}

.sex-or-address {
  position: absolute;
  left: 192rpx;
  top: 16rpx;
  font-size: 12px;
  color: #fff;
  display: flex;
}

.sex-or-address .address {
  background: #868ba4;
  padding: 4rpx 16rpx;
  border-radius: 18rpx;
}

.sex-or-address .sex {
  background: #f77f7f;
  border-radius: 18rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0 16rpx;
  margin-right: 16rpx;
}

.sex-or-address .sex.active {
  background: #59a8ff;
}

.sex-or-address .sex image {
  width: 20rpx;
  height: 20rpx;
  margin-right: 8rpx;
}

.card-info .user-info {
  padding-top: 88rpx;
  font-size: 12px;
  color: #8b9199;
}

.card-info .user-info .user-name {
  font-family: PingFangSC-Medium;
  font-size: 24px;
  color: #2c364c;
}

.card-info .user-position .position {
  font-size: 16px;
  color: #060f38;
}

.card-info .user-position .dian {
  width: 8rpx;
  height: 8rpx;
  display: inline-block;
  border-radius: 50%;
  vertical-align: middle;
  background: #8b9199;
  margin: 0 8rpx;
}

.card-info .user-position .active {
  font-size: 12px;
  color: #8b9199;
  letter-spacing: 0;
}

.card-operating {
  display: flex;
  padding: 48rpx 0;
}

.card-operating .card-operating-button {
  display: flex;
  flex-direction: column;
  align-items: center;
  vertical-align: middle;
  margin-right: 100rpx;
}

.card-operating .card-operating-button image {
  width: 88rpx;
  height: 88rpx;
}

.card-operating .card-operating-button text {
  font-size: 12px;
  color: #060f38;
}

.card-bottom-container {
  margin-top: 32rpx;
  margin-left: 8rpx;
}

.card-bottom-container .card-bottom-img {
  display: flex;
  align-items: center;
}

.card-bottom-container image {
  width: 48rpx;
  height: 48rpx;
  border-radius: 50%;
}

.card-bottom-container .card-popularity-more {
  width: 26rpx;
  height: 6rpx;
  margin-left: 16rpx;
}

.card-bottom-visit-sum text {
  font-size: 12px;
  color: #8b9199;
}

/* -----------------------用户资料----------------------------- */

.detail {
  display: flex;
  flex-direction: column;
  background: #fff;
  margin-top: 16rpx;
}

.detail-title {
  display: flex;
  flex-direction: row;
}

.detail-title-line {
  width: 8rpx;
  height: 40rpx;
  background: #060f38;
  margin-left: 32rpx;
  margin-top: 22rpx;
}

.detail-title-text {
  font-size: 36rpx;
  font-family: PingFangSC-Medium;
  color: #060f38;
  margin-left: 32rpx;
  margin-top: 16rpx;
}

.showinfo-container {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 16rpx;
  padding-bottom: 16rpx;
  margin-left: 48rpx;
  margin-right: 48rpx;
}

.showinfo-icon {
  width: 48rpx;
  height: 48rpx;
}

.showinfo-label {
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  color: #060f38;
  margin-left: 8rpx;
}

.showinfo-content {
  width: 60%;
  display: flex;
  flex-wrap: wrap;
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  color: #8b9199;
}

.showinfo-button {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 112rpx;
  height: 64rpx;
  border: solid #0084ff 2rpx;
  border-radius: 8rpx;
}

.showinfo-button-text {
  color: #0084ff;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
}

.showinfo-bottom-line {
  /* width: 732rpx; */
  margin-left: 48rpx;
  margin-right: 48rpx;
  height: 1rpx;
  background: rgba(0, 0, 0, 0.06);
}

.detail-button {
  margin-top: 48rpx;
  margin-bottom: 48rpx;
  width: 484rpx;
  height: 80rpx;
  font-size: 28rpx;
  color: #fff;
  background: #0084ff;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 40rpx;
  border: none;
}

.bottom-logo {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background: #f2f2f2;
}

.bottom_signature {
  width: 216rpx;
  height: 90rpx;
  margin-top: 32rpx;
  margin-bottom: 46rpx;
}

.bottom-home {
  height: 96rpx;
  width: 100%;
  background: #f7f7fa;
  box-shadow: 0 0 0 0 rgba(0, 0, 0, 0.10);
  font-size: 14px;
  color: #2c364c;
  text-align: center;
  line-height: 96rpx;
}

/* -----------------------我的样式----------------------------- */

.noth-card {
  text-align: center;
  padding-top: 48rpx;
  padding-left: -32rpx;
}

.card-btn {
  position: absolute;
  bottom: -40rpx;
  left: 120rpx;
}

.card-btn button {
  width: 484rpx;
  height: 80rpx;
  font-size: 28rpx;
  color: #fff;
  background: #0084ff;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 40rpx;
  border: none;
}

